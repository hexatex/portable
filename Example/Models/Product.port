Product Model : Sellable
    IsSellable

    relationships
        ProductVariant[] productVariants
            hasMany

    /*
     * Searchable
     */
     public
        json getSearchableArray db
            get {
                return {
                    name: this.name,
                    color: this.color,
                    variants: this.productVariants.each(variant => variant.getSearchableArray()),
                }
            }
